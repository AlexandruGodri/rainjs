{
    "id": "authorization_example",
    "version": "1.0",
    "views": {
        "index" : {
            "view": "index.html",
            "controller": {
                "client": "index.js",
                "server": "index.js"
            }
        },
        "buttons" : {
            "view": "buttons.html",
            "controller": {
                "client": "default.js"
            }
        },
        "button1" : {
            "view": "button1.html",
            "controller": {
                "client": "default.js"
            },
            "permissions": ["view_button1"]
        },
        "button2" : {
            "view": "button2.html",
            "controller": {
                "client": "default.js"
            },
            "permissions": ["view_button2"]
        },
        "button3" : {
            "view": "button3.html",
            "controller": {
                "client": "default.js"
            },
            "permissions": ["view_button3"]
        }
    },
    "intents": []
}
